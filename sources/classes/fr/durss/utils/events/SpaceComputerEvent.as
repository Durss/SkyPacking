package fr.durss.utils.events {	import flash.events.Event;		/**	 * 	 * @author  Francois	 */	public class SpaceComputerEvent extends Event {				public static const SEARCH_START:String		= "searchStart";		public static const SEARCH_COMPLETE:String	= "searchComplete";		public static const SEARCH_PROGRESS:String	= "searchProgress";		private var _percent:Number;		private var _solutions:Array;						/* *********** *		 * CONSTRUCTOR *		 * *********** */		/**		 * Creates an instance of <code>SpaceComputerEvent</code>.<br>		 */		public function SpaceComputerEvent(type:String, solutions:Array = null, percent:Number = 0, bubbles:Boolean = false, cancelable:Boolean = false) {			_solutions = solutions;			_percent = percent;			super(type, bubbles, cancelable);		}						/* ***************** *		 * GETTERS / SETTERS *		 * ***************** */		/**		 * Gets the compute percent done.		 */		public function get percent():Number { return _percent; }				/**		 * Gets the maximum solutions coordinates.		 * 		 * @return an array of <code>Point</code> objects.		 */		public function get solutions():Array { return _solutions; }		/* ****** *		 * PUBLIC *		 * ****** */						/* ******* *		 * PRIVATE *		 * ******* */			}}